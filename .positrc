export RSTUDIO_DISABLE_CRASHPAD=1
export rswrepo="~/git/rstudio-pro"
export rslog="/var/log/rstudio/rstudio-server"
export rswinstall="/usr/lib/rstudio-server"

alias cmrserver="(cd ${rswrepo}/build; cmake -GNinja .. -DRSTUDIO_TARGET=Server -DCMAKE_BUILD_TYPE=Debug -DCMAKE_INSTALL_PREFIX=${rswinstall};)"
alias clrserver="(cd ${rswrepo}/build; ninja clean;)"
alias brserver="(cd ${rswrepo}/build; sudo ninja install -j 10 -v;)"
